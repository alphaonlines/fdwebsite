<!-- Logic --> <script> (function () { function money(n){ return '$' + Number(n).toLocaleString(undefined, { maximumFractionDigits: 0 }); } var cards = document.querySelectorAll('.ms-card'); for (var i = 0; i < cards.length; i++) { var card = cards[i]; var reg = Number(card.getAttribute('data-reg') || 0); var now = Number(card.getAttribute('data-now') || 0); var qty = String(card.getAttribute('data-qty') || '').trim(); // Left badge var badge = card.querySelector('.ms-badge'); var badgeOverride = String(card.getAttribute('data-badge') || '').trim(); if (badge) { if (badgeOverride) badge.textContent = badgeOverride; else badge.textContent = (qty === '1') ? 'Last Ones' : 'Clearance'; } // Top-right badge: amount off var offBadge = card.querySelector('.ms-offbadge'); if (offBadge && reg > 0 && now > 0 && reg > now) { offBadge.textContent = money(reg - now) + ' OFF'; offBadge.style.display = 'inline-block'; } else if (offBadge) { offBadge.textContent = ''; offBadge.style.display = 'none'; } // Under-image line: NOW price var nowLine = card.querySelector('.ms-nowline'); if (nowLine) { if (now > 0) nowLine.textContent = 'Now ' + money(now); else nowLine.textContent = ''; } // Was line value var regLine = card.querySelector('.ms-regline'); if (regLine) { if (reg > 0) regLine.textContent = money(reg); else regLine.textContent = ''; } } })(); </script> <!-- spacer --> <div style="margin: 10px 2px; color: #9aa3b2; font-size: 12px;">Select items only. Limited quantities. Pricing &amp; availability vary by location. Photos are representative; actual product may vary. See store for details.</div> <!-- ================= Why Furniture Distributors + Contact Options ================= --> <div style="max-width: 980px; margin: 26px auto 8px; font-weight: 900; font-size: 22px; letter-spacing: .02em; color: #111827;">ðŸ’› Why Furniture Distributors</div> <table role="presentation" width="100%" cellspacing="0" cellpadding="0" style="max-width: 980px; margin: 0 auto;"> <tbody> <tr> <td w